ast-of-type {\n      width: 195px;\n    }\n\n    .reveal-first, .reveal-second {\n      width: 0;\n      height: 40px;\n      position: absolute;\n      top: 0;\n      left: 0;\n      background-color: #E7174C;\n    }\n\n    .credits {\n      display: inline;\n      position: relative;\n      font-family: barlow-bold;\n      color: #fff;\n      text-decoration: none;\n      font-size: 1em;\n      margin-left: 0;\n      margin-right: 0;\n      padding: 5px 0;\n      z-index: 0;\n      visibility: hidden;\n\n      &.knmi {\n        margin-left: 30px;\n        margin-right: 5px;\n\n        &::after {\n          content: '';\n          position: absolute;\n          left: -30px;\n          top: 0;\n          background: url('../images/gif/powered.gif') no-repeat;\n          z-index: 1;\n          width: 30px;\n          height: 30px;\n          background-size: 20px;\n        }\n      }\n    }\n  }\n}\n\n.flex {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n\n  .container {\n    padding: 0 5vw;\n    max-width: 1280px;\n    margin: 0 auto;\n\n    h1 {\n      position: relative;\n      overflow: hidden;\n      height: 120px;\n      width: 100vw;\n      margin: 0;\n      font-size: 90px;\n      color: #E7174C;\n      font-family: barlow-bold;\n      text-transform: uppercase;\n\n      .hide {\n        position: absolute;\n        z-index: 99;\n        top: 120px;\n        left: 0;\n      }\n    }\n  }\n}\n\n.second-color-slide-up {\n  position: absolute;\n  content: '';\n  left: 0;\n  top: 0;\n  width: 100%;\n  height: 100%;\n  background-color: #ED6A63;\n\n}\n"},"lineCount":null}},"error":null,"hash":"dc4c25cfde635ae59f72730d6cdb8150","cacheData":{"env":{}}}