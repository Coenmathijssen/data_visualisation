0 5vw;\n  background: $brokenwhite;\n  position: relative;\n\n  .chapter {\n    position: relative;\n    overflow: hidden;\n    padding: 10px 0;\n    height: 80px;\n    width: 100vw;\n    font-family: barlow-bold;\n    color: $red;\n    z-index: 10;\n\n    .chapter-appear {\n      position: absolute;\n      z-index: 99;\n      top: 100px;\n      left: 0;\n    }\n  }\n\n  .count {\n    position: fixed;\n    top: -50px;\n    right: 50px;\n    z-index: 9;\n\n    span {\n      color: $red;\n      opacity: 0.1;\n      position: relative;\n       user-select: none;\n    }\n\n    span:first-of-type {\n      left: 30px;\n      font-size: 14em;\n    }\n\n    span:nth-of-type(2) {\n      top: 50px;\n      margin: 0;\n      font-size: 8em;\n      position: relative;\n    }\n\n    span:last-of-type {\n      top: 53px;\n      right: 10px;\n      font-size: 5em;\n      position: relative;\n    }\n  }\n}\n\n\n\n// ASTMA CSS\n\n\n.astma {\n  h2 {\n    color: $red;\n  }\n\n  .explain-text {\n    max-width: 400px;\n    margin-right: 0;\n  }\n\n  .inhaler-container {\n    position: absolute;\n    top: 0;\n    z-index: 10;\n\n    .inhaler-top {\n      position: absolute;\n      content: '';\n      left: 0;\n      top: 0;\n      width: 100px;\n    }\n\n    .inhaler {\n      position: absolute;\n      content: '';\n      left: 11px;\n      top: 40px;\n      width: 165px;\n    }\n  }\n\n  .spray {\n    width: 800px;\n    position: absolute;\n    content: '';\n    transform: rotate(-20deg);\n    left: -290px;\n    bottom: -185px;\n  }\n\n  .spray-container {\n    position: absolute;\n    content: \"\";\n    transform: rotate(-20deg);\n    right: 0;\n    bottom: -150px;\n    width: 830px;\n    height: 760px;\n    overflow: hidden;\n    border-radius: 0px 200px 200px 0px;\n    -moz-border-radius: 0px 200px 200px 0px;\n    -webkit-border-radius: 0px 200px 200px 0px;\n    border: 0px solid #000000;\n  }\n\n  .spray-reveal {\n    position: absolute;\n    content: '';\n    width: 100%;\n    heig