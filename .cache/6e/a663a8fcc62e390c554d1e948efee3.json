2_20190102.json').then(data => {\n  console.log('raw data: ', data)\n  let transformedData = transformData(data)\n  console.log('transformedData: ', transformedData)\n})\n\nconst transformData = (data) => {\n  let transformedDataArray = []\n\n  const chunck = 42128\n\n  let concChunkArray = []\n\n  for (let i = 1; i <= 24; i++) {\n    let concChunk = data.conc_ana.slice(i - 1, chunck * i)\n    concChunkArray.push(concChunk)\n  }\n\n  let day = []\n\n  for (let i = 1; i <= 24; i++) {\n    let hour = {\n      'id': i,\n      'time': data.time[i - 1],\n      'temperature': data.temperature[i - 1],\n      'uWind': data.uWind[i - 1],\n      'vWind': data.vWind[i - 1],\n      'dataArray': []\n    }\n\n    data.lon.forEach((item, index) => {\n      if (index % 100 === 0) {\n        let obj = {\n          'id': (index / 100) + 1,\n          'long': item,\n          'lat': data.lat[index],\n          'conc_ana': concChunkArray[i - 1][index]\n        }\n        hour.dataArray.push(obj)\n      }\n    })\n\n    day.push(hour)\n  }\n\n  transformedDataArray.push(day)\n  return transformedDataArray\n}\n"},"lineCount":null}},"error":null,"hash":"e77f8af9a1643781211cee8aa8f1061a","cacheData":{"env":{}}}